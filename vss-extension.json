{
  "manifestVersion": 1,
  "version": "0.0.85",
  "publisher": "Appcircle",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": [
    "Testing Distribution",
    "Binary Distribution",
    "Beta Distribution",
    "Mobile",
    "iOS",
    "Android",
    "IPA",
    "APK/AAB"
  ],
  "description": "Appcircle Test Distribution allows you to distribute your mobile apps to your test groups",
  "categories": ["Azure Pipelines"],
  "icons": {
    "default": "images/extension-icon.png"
  },
  "files": [
    {
      "path": "buildandreleasetask"
    },
    {
      "path": "images",
      "addressable": true
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "contributions": [
    {
      "id": "custom-build-release-task",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "buildandreleasetask"
      }
    }
  ]
}
