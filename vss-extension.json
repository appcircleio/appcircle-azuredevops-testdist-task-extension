{
  "manifestVersion": 1,
  "id": "build-release-task",
  "name": "Appcircle distribution",
  "version": "0.0.41",
  "publisher": "Appcircle",
  "public": true,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": [
    "distribution",
    "android",
    "ios",
    "beta",
    "beta testing",
    "testing",
    "test",
    "binary",
    "artifact",
    "ipa",
    "apk",
    "mobile"
  ],
  "description": "A suite of tools designed to streamline the distribution of builds to users or testers, simplifying the release process through Appcircle integration.",
  "categories": ["Azure Pipelines"],
  "icons": {
    "default": "images/extension-icon.png"
  },
  "files": [
    {
      "path": "buildandreleasetask"
    },
    {
      "path": "images",
      "addressable": true
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "contributions": [
    {
      "id": "custom-build-release-task",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "buildandreleasetask"
      }
    }
  ]
}
